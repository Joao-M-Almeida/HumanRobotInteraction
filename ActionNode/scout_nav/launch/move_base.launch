<launch>

	<include file="$(find amcl)/examples/amcl_diff.launch" />

	<node pkg="move_base" type="move_base" name="Scout_move_base">
		<rosparam file="$(find scout_nav)/config/move_base/move_base.yaml" command="load"/>
		<rosparam file="$(find scout_nav)/config/move_base/costmap_common_params.yaml" command="load" />
		<rosparam file="$(find scout_nav)/config/move_base/global_costmap_params.yaml" command="load" ns="global_costmap" />
		<rosparam file="$(find scout_nav)/config/move_base/local_costmap_params.yaml" command="load"  ns="local_costmap" />
		<rosparam file="$(find scout_nav)/config/move_base/base_local_planner_params.yaml" command="load"/>
	</node>

</launch>
